<!DOCTYPE html>
<html class="no-js" lang="en">

<head>
    <meta charset="utf-8" />
    <title>Insurance Vehicle</title>
    <link href="~/customer-assets/css/main.css" rel="stylesheet" />
    <link href="~/customer-assets/css/bootstrap.css" rel="stylesheet" />
    <link href="~/customer-assets/css/niceselect.css" rel="stylesheet" />
    <link href="~/customer-assets/css/style.css" rel="stylesheet" />
    <link href="~/customer-assets/css/responsive.css" rel="stylesheet" />
    <link href="~/customer-assets/css/color-switcher-design.css" rel="stylesheet" />

    <link
        href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@300;400;500;600;700;800&amp;family=Ubuntu:wght@300;400;500;700&amp;display=swap"
        rel="stylesheet" />

    <link rel="shortcut icon" href="~/customer-assets/images/favicon.png" type="image/x-icon" />
    <link rel="icon" href="~/customer-assets/images/favicon.png" type="image/x-icon" />
    @RenderSection("Styles", required: false)

    <!-- Responsive -->
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1">

</head>

<body class="aboutus-page">
    <div class="page-wrapper">
        <div class="loading-window-outer">
            <div class="loading-window">
                <div class="car">
                    <div class="strike"></div>
                    <div class="strike strike2"></div>
                    <div class="strike strike3"></div>
                    <div class="strike strike4"></div>
                    <div class="strike strike5"></div>
                    <div class="car-detail spoiler"></div>
                    <div class="car-detail back"></div>
                    <div class="car-detail center"></div>
                    <div class="car-detail center1"></div>
                    <div class="car-detail front"></div>
                    <div class="car-detail wheel"></div>
                    <div class="car-detail wheel wheel2"></div>
                </div>

                <div class="text">
                    <span>Loading</span><span class="dots">...</span>
                </div>
            </div>
        </div>
        @await Html.PartialAsync("_Header")

        <div>
            @RenderBody()
        </div>
        <div class="cantact-palate">
            <div class="calculator-trigger">
                <i class="flaticon-envelope"></i>
            </div>
            <span class="palate-tag">Contact Form</span>
            <div class="palate-content">
                <div class="contact-form">
                    <form method="post" asp-action="Send" asp-controller="Contact" id="contact-form">
                        <div class="form-group">
                            <label for="full_name">Enter Your Name*</label>
                            <input type="text" name="full_name" placeholder="Your Name" required />
                        </div>

                        <div class="form-group">
                            <label for="email">Enter Your Email*</label>
                            <input type="email" name="email" placeholder="Your Email" required />
                        </div>

                        <div class="form-group">
                            <label for="phone">Your Phone*</label>
                            <input type="text" name="phone" placeholder="Your Phone" required />
                        </div>

                        <div class="form-group">
                            <label for="subject">Subject (Optional)</label>
                            <input type="text" name="subject" placeholder="Your Subject" />
                        </div>

                        <div class="form-group">
                            <label for="message">Your Message*</label>
                            <textarea name="message" placeholder="Type Your Message" required></textarea>
                        </div>

                        <div class="form-group">
                            <button class="theme-btn btn-style-one" type="submit" name="submit-form">
                                <span class="txt">Send Now <i class="fa fa-arrow-circle-right"></i></span>
                            </button>
                        </div>
                    </form>

                    @if (TempData["Message"] != null)
                    {
                        <div class="alert alert-success">
                            @TempData["Message"]
                        </div>
                    }

                </div>
            </div>
        </div>
        <footer class="main-footer"
            style="background-image: url('@Url.Content("~/customer-assets/images/background/pattern-2.png")')">
            <div class="auto-container">
                <!-- Upper Box -->
                <div class="upper-box">
                    <div class="row clearfix">
                        <!-- Info Column -->
                        <div class="info-column col-lg-4 col-md-6 col-sm-12">
                            <div class="inner-column">
                                <span class="icon flaticon-call-2"></span>
                                call now<br />
                                <a href="tel:+10909876534">+10909876534</a>
                            </div>
                        </div>

                        <!-- Info Column -->
                        <div class="info-column col-lg-4 col-md-6 col-sm-12">
                            <div class="inner-column">
                                <span class="icon flaticon-email-2"></span>
                                24*7 online Support<br />
                                <a href="mailto:carsore@mail.com">carsore@mail.com</a>
                            </div>
                        </div>

                        <!-- Info Column -->
                        <div class="info-column col-lg-4 col-md-6 col-sm-12">
                            <div class="inner-column">
                                <span class="icon flaticon-maps-and-flags"></span>
                                Office Adress<br />
                                <strong>4309 Terry Lane, USA</strong>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="widgets-section">
                    <div class="row clearfix">
                        <div class="footer-column col-lg-4 col-md-6 col-sm-12">
                            <div class="footer-widget logo-widget">
                                <div class="logo">
                                    <a asp-area="" asp-controller="Home" asp-action="Index"><img
                                            src="~/customer-assets/images/footer-logo.png" alt="" /></a>
                                </div>
                                <div class="text">
                                    Lorem Ipsum is simply dummy text of the printing and
                                    typesetting industry. Lorem Ipsum has been the industry's
                                    standard dummy text.
                                </div>
                                <ul class="social-box">
                                    <li>
                                        <a href="https://www.facebook.com/" class="fab fa-facebook-f"></a>
                                    </li>
                                    <li>
                                        <a href="https://www.twitter.com/" class="fab fa-twitter"></a>
                                    </li>
                                    <li>
                                        <a href="https://google.com/" class="fab fa-google"></a>
                                    </li>
                                    <li>
                                        <a href="https://www.linkedin.com/" class="fab fa-linkedin-in"></a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                        <div class="footer-column col-lg-5 col-md-6 col-sm-12">
                            <div class="footer-widget links-widget">
                                <div class="row clearfix">
                                    <div class="column col-lg-6 col-md-6 col-sm-12">
                                        <h3>Company</h3>
                                        <ul class="list-links">
                                            <li><a href="#">About us</a></li>
                                            <li><a href="#">our team</a></li>
                                            <li><a href="#">Services</a></li>
                                            <li><a href="#">insurance</a></li>
                                            <li><a href="#">blog</a></li>
                                            <li><a href="#">contact us</a></li>
                                        </ul>
                                    </div>
                                    <div class="column col-lg-6 col-md-6 col-sm-12">
                                        <h3>Insurance</h3>
                                        <ul class="list-links">
                                            <li><a href="#">car insurance</a></li>
                                            <li><a href="#">bike insurance</a></li>
                                            <li><a href="#">bus insurance</a></li>
                                            <li><a href="#">mini truck insurance</a></li>
                                            <li><a href="#">jeep insurance</a></li>
                                            <li><a href="#">lorry insurance</a></li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="footer-column col-lg-3 col-md-6 col-sm-12">
                            <div class="footer-widget quote-widget">
                                <h3>Request a Quote</h3>
                                <div class="quote-form">
                                    <form method="post" asp-action="Question" asp-controller="Suport">
                                        <div class="clearfix">
                                            <div class="form-group">
                                                <input type="text" name="username" placeholder="Your Name *"
                                                    required="" />
                                            </div>

                                            <div class="form-group">
                                                <input type="email" name="email" placeholder="Your Email" required="" />
                                            </div>

                                            <div class="form-group">
                                                <select class="custom-select-box nice-select" name="subject">
                                                    <option value="Giá Bảo Hiểm xe Ô tô">Giá Bảo Hiểm xe Ô tô</option>
                                                    <option value="Giá Bảo Hiểm xe Mô tô">Giá Bảo Hiểm xe Mô tô</option>
                                                    <option value="Giá Bảo Hiểm Dành Cho Xe Tải">Giá Bảo Hiểm Dành Cho
                                                        Xe Tải</option>
                                                </select>
                                            </div>

                                            <div class="form-group">
                                                <button class="theme-btn submit-btn" type="submit" name="submit-form">
                                                    Submit Now
                                                </button>
                                            </div>
                                        </div>
                                    </form>


                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </footer>
    </div>

    <div class="scroll-to-top scroll-to-target" data-target="html">
        <span class="fa fa-arrow-up"></span>
    </div>

    <script src="~/customer-assets/js/icon.js"></script>
    <script src="~/customer-assets/js/jquery.js"></script>
    <script src="~/customer-assets/js/popper.min.js"></script>
    <script src="~/customer-assets/js/bootstrap.min.js"></script>
    <script src="~/customer-assets/js/jquery.fancybox.js"></script>
    <script src="~/customer-assets/js/appear.js"></script>
    <script src="~/customer-assets/js/parallax.min.js"></script>
    <script src="~/customer-assets/js/tilt.jquery.min.js"></script>
    <script src="~/customer-assets/js/jquery.paroller.min.js"></script>
    <script src="~/customer-assets/js/owl.js"></script>
    <script src="~/customer-assets/js/wow.js"></script>
    <script src="~/customer-assets/js/nav-tool.js"></script>
    <script src="~/customer-assets/js/jquery-ui.js"></script>
    <script src="~/customer-assets/js/validate.js"></script>
    <script src="~/customer-assets/js/jquery.nice-select.min.js"></script>
    <script src="~/customer-assets/js/jquery.mCustomScrollbar.concat.min.js"></script>
    <script src="~/customer-assets/js/script.js"></script>
    <script src="~/customer-assets/js/color-settings.js"></script>
    <script src="~/customer-assets/js/pay.js"></script>

</body>

</html>